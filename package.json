  "scripts": {
    "link": "npm link --force",
    "lint": "eslint .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testMatch='**/test/**/*.test.js'",
    "test-logger": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testMatch='**/test/**/logger/*.test.js'",
